type User {
  id: ID!
  username: String!
}

type Query {
  userById(id: ID): User
  getAllUsers: [User]
  test: String
}

type Gallery {
  id: ID!
  title: String!
  creator: String!
  isShared: Boolean
}

type Canvas {
  id: ID!
  title: String!
  creator: String!
  thumbnail: String
  isShared: Boolean
  collaborators: [User]
}